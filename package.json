{
  "name": "kiwii",
  "version": "0.4.1",
  "description": "A lightweight HTML5 Hybrid Mobile App framework",
  "main": "index.js",
  "bin": {
    "kiwii": "./bin/kiwii.js"
  },
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp",
    "app": "gulp connect",
    "dist": "gulp connectdist",
    "pre-commit": "npm run makedist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/kiwii.git"
  },
  "keywords": [
    "hybrid mobile",
    "template",
    "cordova",
    "backbone",
    "es6",
    "babel",
    "browserify",
    "ios"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/kiwii/issues"
  },
  "homepage": "https://github.com/jclo/kiwii",
  "dependencies": {
    "backbone": "1.3.3",
    "backbone.radio": "2.0.0",
    "font-awesome": "4.7.0",
    "goratchet": "2.0.2",
    "lodash": "4.17.10",
    "nopt": "4.0.1",
    "opn": "5.3.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "weathericons": "2.1.0",
    "zepto": "1.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babelify": "8.0.0",
    "browserify": "16.2.2",
    "browserify-shim": "3.8.14",
    "browserify-swap": "0.2.2",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "gulp": "3.9.1",
    "gulp-clean-css": "3.9.4",
    "gulp-concat": "2.6.1",
    "gulp-connect": "5.5.0",
    "gulp-header": "2.0.5",
    "gulp-rename": "1.3.0",
    "gulp-replace": "1.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "husky": "0.14.3",
    "path": "0.12.7",
    "rename": "1.0.4",
    "require-dir": "1.0.0",
    "run-sequence": "2.2.1",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.11.0"
  },
  "browser": {
    "zepto": "./node_modules/zepto/dist/zepto.js",
    "ratchet": "./node_modules/goratchet/dist/js/ratchet.js"
  },
  "browserify-shim": {
    "zepto": "$",
    "ratchet": {
      "exports": "Ratchet"
    }
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "dist": {
      "underscore.js$": "lodash"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "browserify-swap"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
